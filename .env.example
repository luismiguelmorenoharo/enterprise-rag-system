# RAG Config
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
TOP_K_RETRIEVAL=5
SIMILARITY_THRESHOLD=0.7

# Database
REDIS_URL=redis://localhost:6379

# Paths
UPLOAD_DIR=data/uploads
PROCESSED_DIR=data/processed
